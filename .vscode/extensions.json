{
  "name": "locust-helper",
  "displayName": "Locust Helper",
  "description": "VSCode extension to refresh Locust scenarios",
  "version": "0.0.1",
  "publisher": "Emil Karlsson",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:locust.refreshScenarios"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "locust.refreshScenarios",
        "title": "Locust: Refresh Scenarios"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "locustScenarios",
          "name": "Locust Scenarios"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "18.x",
    "typescript": "^5.2.0",
    "tslint": "^6.1.3"
  }
}
